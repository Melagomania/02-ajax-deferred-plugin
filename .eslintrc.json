{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": 1,
        "no-debugger": 1,
        "no-empty": 1,
        "no-constant-condition": 2,
        "curly": 1,
        "default-case": 1,
        "no-unused-vars": 1,
        "eqeqeq": 1,
        "no-alert": 1,
        "no-global-assign": 1,
        "no-empty-function": 1,
        "no-eq-null": 2,
        "yoda": 2,
        "no-useless-return": 1,
        "no-use-before-define": 1
    }
}